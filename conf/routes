GET     /                           controllers.HomeController.index

GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

GET     /login                      controllers.HomeController.login

GET     /register                   controllers.HomeController.register

POST    /registerHandler            controllers.HomeController.registrationHandler

POST    /loginHandler               controllers.HomeController.loginHandler

POST    /adminAuth                   controllers.HomeController.adminAuth

GET    /admin                       controllers.HomeController.adminIndex

GET    /enroll                      controllers.HomeController.enroll